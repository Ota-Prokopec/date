{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/**/*.js",
  "exports": {
    "./actions/*": "./dist/actions/*.js",
    "./components/*": "./src/components/*.tsx",
    "./dist/*": "./dist/*.js",
    "./ts-lib/*": "./src/*.ts",
    "./ts-react/*": "./src/*.tsx",
    "./contexts/*": "./src/contexts/*.tsx",
    "./hooks/*": "./dist/hooks/*.js",
    "./lib/*": "./dist/lib/*.js",
    "./tailwindConfig": "./tailwind.config.ts",
    "./tailwindcss": "./tailwindcss.css"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "run-s build:tsup build:tsc",
    "build:tsup": "NODE_OPTIONS='--max-old-space-size=16384' tsup",
    "dev": "run-p dev:tsup",
    "dev:tsup": "NODE_OPTIONS='--max-old-space-size=16384' tsup --watch",
    "dev:tsc": "tsc --watch --emitDeclarationOnly",
    "generate:components": "barrelsby --delete  --singleQuotes --directory ./src",
    "build:tsc": "tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/ts-types": "workspace:*",
    "@repo/tsup": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "^4.0.4",
    "@turbo/gen": "^1.12.4",
    "@types/lodash": "^4.17.10",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-avatar-editor": "^13.0.3",
    "@types/react-dom": "^19",
    "barrelsby": "^2.8.0",
    "daisyui": "^4.12.14",
    "eslint": "^9.20.0",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.49",
    "tailwindcss": "^3.4.17",
    "tsup": "8.2.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@heroui/react": "^2.7.2",
    "@hookform/resolvers": "^4.0.0",
    "@internationalized/date": "^3.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@remix-run/node": "^2.15.0",
    "@repo/color-theme": "workspace: *",
    "@repo/cookies": "workspace: *",
    "@repo/env": "workspace: *",
    "@repo/i18n-next": "workspace: *",
    "@repo/local-storage": "workspace: *",
    "@repo/utils": "workspace: *",
    "@tanstack/react-query": "^5.59.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "eslint-config-next": "15.1.6",
